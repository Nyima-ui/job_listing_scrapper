{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Tenzin's Proxy Auth",
  "permissions": [
    "proxy",
    "tabs",
    "unlimitedStorage",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "incognito": "split"
}
